 : \"hidden\",\n       \n\n       }\n\n       function handleClick(event){\n        //console.log(event.target.innerText)\n        const option = event.target.innerText\n        props.getTask(option)\n       }\n\n    return(\n\n        <div className='StartMenu' style = {menuStyle}>\n            <div className='StartMenu-header'>\n                <div className='StartMenu-DP'></div>\n            </div>\n            <ul className='StartMenu-list'>\n                <li className='StartMenu-list-item' onClick={handleClick}>\n                    <div className='list-item-wrapper'>\n                        <p> <FontAwesomeIcon icon={faQuestion}/> About</p>\n                    </div>\n                </li>\n                <li className='StartMenu-list-item' onClick={handleClick}>\n                <div className='list-item-wrapper'>\n                        <p> <img src='github-logo.png' height=\"20px\"/> Github Profile</p>\n                    </div>\n                </li>\n                <li className='StartMenu-list-item' onClick={handleClick}>\n                <div className='list-item-wrapper'>\n                        <p> <FontAwesomeIcon icon={faFolder}/> All Projects</p>\n                    </div>\n                </li>\n                \n\n            </ul>\n\n        </div>\n\n\n\n    )\n}","/Users/lordsmac/Documents/Dev/React_Play_Ground/Dashboard/Portfolio/portfolio/src/components/Window.js",["82","83","84"],[],"import React, { Component } from 'react';\nimport { faMagnifyingGlass, faFolderOpen,  faQuestion} from \"@fortawesome/free-solid-svg-icons\";\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome';\n\n\nexport default function Window(props){\n    \n\n    \n    const windowStyle = {\n        visibility : props.visibility ? \"visible\" : \"hidden\",\n       }\n\n    function minimise(){\n        props.toggle()\n    }\n\n    function close(){\n        props.close()\n    }\n  \n\n    return(\n        <div className='Window' style = {windowStyle}>\n            <div className='top-menu'>\n                <h5><FontAwesomeIcon icon={faFolderOpen}/> {props.title}</h5>\n                <div className='top-menu-buttons'>\n                <button className='minimise'onClick={minimise}>-</button>\n                <button className='maximise'>H</button>\n                <button className='close' onClick={close}>X</button>\n                </div>\n            </div>\n            <div className='inner-menu'>\n                <div className='inner-menu-container'></div>\n                <div className='search-bar-container'>\n                    <p>Search</p>\n                <div className='search-bar'>\n                    <p>{props.title}</p>\n                </div>\n                </div>\n            </div>\n            <div className='inner-wrapper'></div>\n            \n        </div>\n    )\n}",[],"/Users/lordsmac/Documents/Dev/React_Play_Ground/Dashboard/Portfolio/portfolio/src/components/WeatherWidget.js",["85","86"],[],"import React, { Component, Suspense } from 'react';\n\n\n\nexport default function WeatherWidget(props){\n\n\n    return(\n        <div className='Widget'>\n          \n           <h3 className='city'>{props.data.location.name}</h3>\n           <p className='city'>{props.data.location.region}</p>\n           <p className='city'>{props.data.location.country}</p>\n           <p className='city'>{props.data.location.localtime.split(\" \")[0]}</p>\n           \n            \n            <div className='weather-info'> <p>{props.data.current.condition.text} </p><img className=\"icon\" alt=\"icon\" src={props.data.current.condition.icon}/></div>\n            <p className='weather-info'> Temperature : {props.data.current.temp_c} C</p>\n            <p className='weather-info'> Humidity : {props.data.current.humidity}</p>\n            <p className='weather-info'> Wind Speed : {props.data.current.wind_kph} kph</p>\n            \n        </div>\n    )\n\n}","/Users/lordsmac/Documents/Dev/React_Play_Gr